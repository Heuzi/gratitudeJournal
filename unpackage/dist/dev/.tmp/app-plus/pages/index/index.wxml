<view class="full-page"><view class="padding flex flex-wrap justify-between align-end bg-white"><button data-event-opts="{{[['tap',[['settings',['$event']]]]]}}" class="cu-btn round" bindtap="__e">Settings</button><button data-event-opts="{{[['tap',[['examples',['$event']]]]]}}" class="cu-btn round" bindtap="__e">Gratitude Examples</button><button data-event-opts="{{[['tap',[['about',['$event']]]]]}}" class="cu-btn round" bindtap="__e">About</button></view><view class="uni-list"><view class="uni-list-cell" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['newjournal',['$event']]]]]}}" bindtap="__e"><view class="uni-media-list-body"><view class="uni-media-list-text-top" type="primary">+ New Journal</view><view class="uni-media-list-text-bottom uni-ellipsis">Date will be automatically set to today's date</view></view></view><block wx:for="{{journals}}" wx:for-item="journal" wx:for-index="index" wx:key="index"><view class="uni-list-cell" hover-class="uni-list-cell-hover" data-id="{{journal.id}}" data-event-opts="{{[['tap',[['openJournal',['$event']]]]]}}" bindtap="__e"><view class="uni-media-list-body"><view class="uni-media-list-text-top">{{journal.title}}</view><view class="uni-media-list-text-bottom uni-ellipsis">{{journal.date}}</view></view></view></block></view></view>